------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "MyProject/src/\\(default package\\)/Class1.java"
    get-item "MyProject/src/\\(default package\\)/Class1.java" | drag-start 15 10
    get-item "MyProject/src/\\(default package\\)/Class1.java" | drag-enter 16 5 -detail 1 -operation 7
    get-item "MyProject/src/\\(default package\\)/Class1.java" | drag-over 16 -operation 7
    get-item "MyProject/src/\\(default package\\)" | drag-over 35 10 -detail 1 -operation 7
    drag-exit
    get-item "MyProject/src/\\(default package\\)" | drag-accept 35 10 -detail 1 -operation 7
    drag-set-data
    get-item "MyProject/src/\\(default package\\)" | drop 35 10 -detail 1 -operation 7
}
with [get-window "Name Conflict"] {
    get-editbox -after [get-label "Enter a new name for \'Class1\':"] | set-focus
    get-button OK | click
}
get-view "Package Explorer" | get-tree | drag-end -detail 1
with [get-view "Package Explorer" | get-tree] {
    get-item "MyProject/src/\\(default package\\)" | get-property childCount | equals 3 | verify-true
    get-item "MyProject/src/\\(default package\\)/Class1.java" | get-property caption | equals "Class1.java" 
        | verify-true
    get-item "MyProject/src/\\(default package\\)/Class2.java" | get-property caption | equals "Class2.java" 
        | verify-true
    get-item "MyProject/src/\\(default package\\)/CopyOfClass1.java" | get-property caption 
        | equals "CopyOfClass1.java" | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
