Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/src/\\(default package\\)/Class1.java" 
    | get-menu "Export..." | click
with [get-window Export] {
    get-tree | select "General/Archive File"
    get-button "Next >" | click
}
set-dialog-result File "platform:/resource/MyProject/rrr.zip"
with [get-window Export] {
    get-button "Browse..." | click
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | select "MyProject/src/\\(default package\\)/Class1.java" | key-type Del
get-window "Confirm Delete" | get-button OK | click
get-view "Package Explorer" | get-tree | select "MyProject/src/\\(default package\\)" | get-menu "Import..." | click
with [get-window Import] {
    get-tree | select "General/File System"
    get-button "Next >" | click
}
set-dialog-result Folder "platform:/resource/MyProject"
with [get-window Import] {
    get-button "Browse..." | click
    get-table | get-item "rrr.zip" | check
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | get-item "MyProject/src/rrr.zip" | get-property caption | equals "rrr.zip" 
    | verify-true
