------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/F2" | get-menu "Refactor/Rename..." | click
with [get-window "Rename Resource"] {
    get-editbox -after [get-label "New name:"] | set-text "F2_renamed"
    get-button OK | click
}
get-view "Package Explorer" | get-tree | get-item "MyProject/F2_renamed" | get-property caption | equals "F2_renamed" 
    | verify-true
get-view "Package Explorer" | get-tree | select "MyProject/F2_renamed"
