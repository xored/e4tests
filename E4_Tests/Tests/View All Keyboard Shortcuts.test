------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/src/\\(default package\\)/Class1.java" 
    | select "MyProject/src/\\(default package\\)/Class1.java" | double-click
with [get-editor "Class1.java"] {
    click
    with [get-text-viewer] {
        set-caret-pos 5 1
        key-type "M1+M2+l"
    }
}
with [get-window -from "Window.createShell()" | get-table] {
    select "Close All" | click -default
   
}
get-editor "Class1.java" | get-text-viewer | set-caret-pos 5 1
