------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/src/File1" | double-click
get-editor File1 | click
get-editor File1 | get-editbox | get-property text | equals "This is a text of File1\n\n"
    + "1111\n"
    + "2222\n"
    + "3333\n"
    + "4444" | verify-true
get-editor File1 | get-text-viewer | set-caret-pos 4 1
get-editor File1 | get-editbox | key-type "M1+M3+ARROW_UP"
get-editor File1 | get-editbox | get-property text | equals "This is a text of File1\n\n"
    + "1111\n"
    + "2222\n"
    + "2222\n"
    + "3333\n"
    + "4444" | verify-true
get-editor File1 | get-editbox | set-caret-pos 6 1
get-editor File1 | get-editbox | key-type "M1+M3+ARROW_DOWN"
get-editor File1 | get-editbox | get-property text | equals "This is a text of File1\n\n"
    + "1111\n"
    + "2222\n"
    + "2222\n"
    + "3333\n"
    + "3333\n"
    + "4444" | verify-true
