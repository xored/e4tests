------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select MyProject | get-menu "New/Folder" | click
with [get-window "New Folder"] {
    get-editbox -after [get-label "Folder name:"] | set-text Folder1
    get-button Finish | click
}
get-view "Package Explorer" | get-tree | get-item "MyProject/Folder1" | get-property caption | equals Folder1 
    | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
