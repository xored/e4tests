------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-menu "Window/Show View/Other..." | click
with [get-window "Show View"] {
    get-tree | select "Ant/Ant"
    get-button OK | click
}
get-menu "Window/Save Perspective As..." | click
with [get-window "Save Perspective As..."] {
    get-table | select Resource
    get-button OK | click
    get-window "Overwrite Perspective" | get-button Yes | click
}
get-menu "Window/Open Perspective/Other..." | click
get-window "Open Perspective" | get-button Cancel | click
get-preferences-menu | click
with [get-window Preferences] {
    get-tree | select "General/Perspectives"
    get-table | select Resource
    get-button Revert | click
    get-button Apply | click
    get-button OK | click
}
get-menu "Window/Reset Perspective..." | click
get-window "Reset Perspective" | get-button No | click
with [get-button Resource] {
    click
    click
}
get-menu "Window/Reset Perspective..." | click
get-window "Reset Perspective" | get-button Yes | click
get-tab-folder -index 1 | get-property tabCount | equals 1 | verify-true
get-tab-folder | get-property activePage | equals "Project Explorer" | verify-true
get-tab-folder -index 1 | get-property activePage | equals Outline | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
