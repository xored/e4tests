get-menu "Navigate/Open Resource..." | click
with [get-window "Open Resource"] {
    get-editbox -after [get-label "Select an item to open (? = any character, * = any string):"] | set-text c
    get-table | select "Class1.java"
    get-editbox -after [get-label "Select an item to open (? = any character, * = any string):"] | set-text cl
    get-table | select "Class1.java"
    get-editbox -after [get-label "Select an item to open (? = any character, * = any string):"] | set-text cla
    get-table | select "Class1.java"
}
with [get-window "Open Resource" | get-table] {
    get-property itemCount | equals 2 | verify-true
    get-item "Class1.java" | get-property caption | equals "Class1.java" | verify-true
    get-item "Class2.java" | get-property caption | equals "Class2.java" | verify-true
}
get-window "Open Resource" | get-button Open | click
get-editor "Class1.java" | click
get-tab-folder -index 1 | get-property activePage | equals "Class1.java" | verify-true
