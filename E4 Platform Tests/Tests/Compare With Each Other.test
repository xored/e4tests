------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "MyProject/src/\\(default package\\)/Class1.java" "MyProject/src/\\(default package\\)/Class2.java"
    get-menu "Compare With/Each Other" | click
}
get-editor "Compare (\'MyProject/src/Class1.java\' - \'MyProject/src/Class2.java\')" | click
with [get-editor "Compare (\'MyProject/src/Class1.java\' - \'MyProject/src/Class2.java\')"] {
    get-editbox -after [get-label "MyProject/src/Class2.java"] | get-property text | equals "public class Class1 {\n\n"
        + "}" | verify-true
    get-editbox -after [get-label "MyProject/src/Class2.java"] -index 1 | get-property text 
        | equals "public class Class2 {\n\n"
        + "}" | verify-true
    get-tree | get-property itemCount | equals 1 | verify-true
}
get-tab-folder -index 1 | get-property activePage 
    | equals "Compare (\'MyProject/src/Class1.java\' - \'MyProject/src/Class2.java\')" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
