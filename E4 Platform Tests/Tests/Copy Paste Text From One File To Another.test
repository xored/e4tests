------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/File1" | double-click
with [get-editor File1 | get-text-viewer] {
    set-caret-pos 6 5
    key-type "M1+a"
    key-type "M1+c"
}
get-view "Package Explorer" | get-tree | select MyProject | get-menu "New/File" | click
with [get-window "New File"] {
    get-editbox -after [get-label "File name:"] | set-text f3
    get-button Finish | click
}
get-editor f3 | get-text-viewer | key-type "M1+v"
get-editor f3 | get-editbox | get-property text | equals "This is a text of File1\n\n"
    + "1111\n"
    + "2222\n"
    + "3333\n"
    + "4444" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
