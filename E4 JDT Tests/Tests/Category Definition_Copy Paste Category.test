------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "PluginProject/CatDef1.xml" | double-click
    select "PluginProject/CatDef2.xml" | double-click
}
get-editor "CatDef2.xml" | get-section "Managing the Categories" | get-tree | select category1 | get-menu Copy | click
with [get-editor "CatDef1.xml" | get-section "Managing the Categories" | get-tree] {
    get-menu Paste | click
    select category1
}
with [get-editor "CatDef1.xml" | get-section "Category Properties"] {
    get-editbox -after [get-label "ID*:"] | get-property text | equals category1 | verify-true
    get-editbox -after [get-label "Name*:"] | get-property text | equals category1 | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
