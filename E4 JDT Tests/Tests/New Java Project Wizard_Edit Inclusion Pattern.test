------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | get-menu "New/Java Project" | click
with [get-window "New Java Project"] {
    get-editbox -after [get-label "Project name:"] | set-text Project
    get-button "Next >" | click
    get-tree | select "Project/src"
    get-button "Configure Inclusion / Exclusion Filters" | click
}
with [get-window "Inclusion and Exclusion Patterns"] {
    get-button "Add..." | click
    with [get-window "Add Inclusion Pattern"] {
        get-editbox -after [get-label "Inclusion pattern (Path relative to \'Project/src\'):"] | set-text "a*"
        get-button OK | click
    }
}
get-window "Inclusion and Exclusion Patterns" | get-table | get-item "a\\*" | get-property caption | equals "a*" 
    | verify-true
with [get-window "Inclusion and Exclusion Patterns"] {
    get-button "Edit..." | click
    with [get-window "Edit Inclusion Pattern"] {
        get-editbox -after [get-label "Inclusion pattern (Path relative to \'Project/src\'):"] | set-text "aaaa*"
        get-button OK | click
    }
}
get-window "Inclusion and Exclusion Patterns" | get-table | get-item "aaaa\\*" | get-property caption | equals "aaaa*" 
    | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
