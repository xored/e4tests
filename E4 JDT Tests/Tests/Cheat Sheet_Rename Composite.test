------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/CompositeCheatSheet.xml" | double-click
get-view "Package Explorer" | get-tree | select "MyProject/CompositeCheatSheet.xml" | key-type F2
with [get-window "Rename Resource"] {
    with [get-editbox -after [get-label "New name:"]] {
        click-text 19
        key-type Right
        set-text "CompositeCheatSheet2.xml"
    }
    get-button OK | click
}
get-view "Package Explorer" | get-tree | select "MyProject/CompositeCheatSheet2.xml" | double-click
get-tab-folder -index 1 | get-property activePage | equals "CompositeCheatSheet2.xml" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
