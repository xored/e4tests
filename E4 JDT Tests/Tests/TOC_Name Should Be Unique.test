------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select MyProject | get-menu "New/Other..." | click
with [get-window New] {
    get-tree | select "User Assistance/Help Table of Contents"
    get-button "Next >" | click
}
with [get-window "New table of contents"] {
    get-tree | select MyProject
    get-editbox -after [get-label "File name:"] | set-text "TOC.xml"
}
with [get-window "New table of contents"] {
    get-editbox -after [get-label "Table of contents"] | get-property text | equals "\'TOC.xml\' already exists." 
        | verify-true
    get-button Finish | get-property enablement | equals false | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
