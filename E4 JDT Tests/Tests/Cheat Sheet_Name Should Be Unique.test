------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "MyProject/SimpleCheatSheet.xml" | double-click
    select "MyProject/SimpleCheatSheet.xml" | get-menu "New/Other..." | click
}
with [get-window New] {
    with [get-tree] {
        select "User Assistance" | double-click
        select "User Assistance/Cheat Sheet"
    }
    get-button "Next >" | click
}
get-window "New Cheat Sheet" | get-editbox -after [get-label "File name:"] | set-text "SimpleCheatSheet.xml"
with [get-window "New Cheat Sheet"] {
    get-editbox -after [get-label "Cheat Sheet"] | get-property text 
        | equals "\'SimpleCheatSheet.xml\' already exists." | verify-true
    get-button Finish | get-property enablement | equals false | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
