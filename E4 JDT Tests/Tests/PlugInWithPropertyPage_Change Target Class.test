------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | get-menu "New/Plug-in Project" | click
with [get-window "New Plug-in Project"] {
    get-editbox -after [get-label "Project name:"] | set-text MyProject
    with [get-button "Next >"] {
        click
        click
    }
    get-table | select "Plug-in with a property page"
    get-button "Next >" | click
}
with [get-window "New plug-in project with a property page"] {
    with [get-editbox -after [get-label "Target Class:"]] {
        set-text "org.eclipse.core.resources"
        key-type "M1+SPACE"
        set-text "org.eclipse.core.resources "
        key-type "M1+SPACE"
        set-text "org.eclipse.core.resources"
    }
    get-button Finish | click
}
with [get-editor MyProject] {
    get-button "Show List" | click
    get-tab-folder | get-tab-item "plugin.xml" | click
}
get-editor MyProject | get-editbox | get-property text | contains "value=\"org.eclipse.core.resources\"" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
