------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/Page1.jpage" | double-click
with [get-editor "Page1.jpage" | get-text-viewer] {
    set-caret-pos 1 20
    key-type Enter
    type-text "System."
    show-content-assist
    type-text "out."
    show-content-assist
    type-text "println()"
    key-type Left
    type-text "\"\""
    key-type Left
    type-text "Hello, World!"
    key-type Right -times 2
    type-text ";"
    
}
with [get-editor "Page1.jpage" | get-text-viewer] {
    set-caret-pos 2 36
    select-range 2 37 2 1
}
get-button "Execute the Selected Text" -index 1 | click
get-view Console | get-editbox | get-property text | equals "Hello, World!" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
