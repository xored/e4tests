------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | get-menu "New/Java Project" | click
with [get-window "New Java Project"] {
    get-editbox -after [get-label "Project name:"] | set-text P
    get-button "Next >" | click
    get-tab-folder | get-tab-item Projects | click
    get-button "Add..." | click
    get-window "Required Project Selection" | get-button "Select All" | click
}
with [get-window "New Java Project" | get-window "Required Project Selection" | get-table] {
    get-property itemCount | equals 2 | verify-true
    get-item MyProject | get-property checked | equals true | verify-true
    get-item PluginProject | get-property checked | equals true | verify-true
}
get-window "New Java Project" | get-window "Required Project Selection" | get-button OK | click
with [get-window "New Java Project" | get-tree] {
    get-property itemCount | equals 2 | verify-true
    get-item MyProject | get-property caption | equals MyProject | verify-true
    get-item PluginProject | get-property caption | equals PluginProject | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
