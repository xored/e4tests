------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/TOC.xml" | double-click
with [get-editor "TOC.xml"] {
    get-section "Table of Contents" | get-tree | select Book
    with [get-section "Book Details" | get-editbox -after [get-label "Name:*"]] {
        set-text "Book_new"
        key-type "M1+s"
    }
}
get-editor "TOC.xml" | get-section "Book Details" | get-editbox -after [get-label "Name:*"] | get-property text 
    | equals "Book_new" | verify-true
get-editor "TOC.xml" | close
get-view "Package Explorer" | get-tree | select "MyProject/TOC.xml" | double-click
get-editor "TOC.xml" | get-section "Table of Contents" | get-tree | select "Book_new"
get-view Outline | get-tree | get-item "Definition/Book_new" | get-property caption | equals "Book_new" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
