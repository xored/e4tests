TEST STEPS

1. Open New Java Project Wizard;
2. On a 2nd page select Projects tab and press Add button, select MyProject;
3. Remove MyProject from a build path with "Remove" button.
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select MyProject | get-menu "New/Java Project" | click
with [get-window "New Java Project"] {
    get-editbox -after [get-label "Project name:"] | set-text P
    get-button "Next >" | click
    get-tab-folder | get-tab-item Projects | click
    get-button "Add..." | click
    with [get-window "Required Project Selection"] {
        get-table | get-item MyProject | check
        get-button OK | click
    }
}
with [get-window "New Java Project" | get-tree] {
    get-property itemCount | equals 1 | verify-true
    get-item MyProject | get-property caption | equals MyProject | verify-true
}
with [get-window "New Java Project"] {
    get-tree | select MyProject
    get-button Remove | click
}
get-window "New Java Project" | get-tree | get-property itemCount | equals 0 | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
