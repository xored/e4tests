with [get-editor "CompositeCheatSheet.xml"] {
    get-section Content | get-tree | select "Title123/Group"
    with [get-section Definition | get-button "This group is optional"] {
        click
        key-type "M1+s"
    }
    get-section Content | get-button Preview | click
}
get-view "Cheat Sheets" | get-link "Skip this group of tasks" | click
