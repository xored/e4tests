------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/ContextHelp.xml" | double-click
get-editor "ContextHelp.xml" | get-section "Context Help" | get-tree | select "Context/Help_Command"
get-editor "ContextHelp.xml" | get-section "Context Help" | get-tree | get-item "Context/Help_Command" 
    | get-property index | equals 3 | verify-true
with [get-editor "ContextHelp.xml" | get-section "Context Help"] {
    get-tree | select "Context/Help_Command"
    with [get-button Up] {
        click
        click
    }
}
get-editor "ContextHelp.xml" | get-section "Context Help" | get-tree | get-item "Context/Help_Command" 
    | get-property index | equals 1 | verify-true
