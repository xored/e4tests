------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyFeatureProject/component.xml" | double-click
with [get-editor "component.xml" | get-section "Properties (9)"] {
    get-table -index 1 | select byteProperty characterProperty doubleProperty floatProperty
    get-button Remove -index 1 | click
}
get-editor "component.xml" | get-section "Properties (5)" | get-table -index 1 | get-property itemCount | equals 5 
    | verify-true
