------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "MyProject/SimpleCheatSheet.xml" | get-menu "Refactor/Rename..." | click
with [get-window "Rename Resource"] {
    get-editbox -after [get-label "New name:"] | set-text "SimpleCheatSheet2.xml"
    get-button OK | click
}
get-view "Package Explorer" | get-tree | select "MyProject/SimpleCheatSheet2.xml" | double-click
get-tab-folder -index 1 | get-property activePage | equals "SimpleCheatSheet2.xml" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
