------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "PluginProject/CatDef2.xml" | double-click
with [get-editor "CatDef2.xml"] {
    get-section "Managing the Categories" | get-tree | select category1
    with [get-section "Category Properties" | get-editbox -after [get-label "Description:"]] {
        set-text text
        key-type "M1+a"
        key-type "M1+c"
        
    }
    get-section "Managing the Categories" | get-tree | select category3 | double-click
    with [get-section "Category Properties" | get-editbox -after [get-label "Description:"]] {
        click-text
        key-type "M1+v"
        
    }
}
get-editor "CatDef2.xml" | get-section "Category Properties" | get-editbox -after [get-label "Description:"] 
    | get-property text | equals text | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
