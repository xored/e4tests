------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | get-menu "New/Feature Project" | click
get-window "New Feature" | get-editbox -after [get-label "Project name:"] | set-text "MyProject.qqq"
get-window "New Feature" | get-group "Feature properties" | get-editbox -after [get-label "Feature ID:"] 
    | get-property text | equals "MyProject.qqq" | verify-true
with [get-window "New Feature"] {
    get-button "Next >" | click
    get-button Finish | click
}
get-editor "MyProject.qqq" | get-section "General Information" | get-editbox -after [get-label "ID:"] | set-focus
get-editor "MyProject.qqq" | get-section "General Information" | get-editbox -after [get-label "ID:"] 
    | get-property text | equals "MyProject.qqq" | verify-true
